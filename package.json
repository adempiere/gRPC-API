{
  "name": "@adempiere/grpc-api",
  "version": "2.4.4",
  "description": "ADempiere Web Store write in Javascript for a node service",
  "author": "Yamel Senih",
  "contributors": [
    {
      "name": "Edwin Betancourt",
      "email": "EdwinBetanc0urt@outlook.com",
      "url": "https://github.com/EdwinBetanc0urt/"
    }
  ],
  "main": "index.js",
  "scripts": {
    "stub": "npm run stub:access && npm run stub:base_data_type && npm run stub:business && npm run stub:client && npm run stub:core_functionality && npm run stub:dictionary && npm run stub:enrollment && npm run stub:point_of_sales",
    "stub:access": "grpc_tools_node_protoc proto/access.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:base_data_type": "grpc_tools_node_protoc proto/base_data_type.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:business": "grpc_tools_node_protoc proto/business.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:client": "grpc_tools_node_protoc proto/client.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:core_functionality": "grpc_tools_node_protoc proto/core_functionality.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:dictionary": "grpc_tools_node_protoc proto/dictionary.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:enrollment": "grpc_tools_node_protoc proto/enrollment.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "stub:point_of_sales": "grpc_tools_node_protoc proto/point_of_sales.proto --js_out=import_style=commonjs,binary:src/grpc/ --grpc_out=grpc_js:src/grpc/",
    "sync": "npm run sync:access && npm run sync:base_data_type && npm run sync:business && npm run sync:client && npm run sync:core_functionality && npm run sync:dictionary && npm run sync:enrollment && npm run sync:point_of_sales",
    "sync:access": "curl --output ./proto/access.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/access.proto'",
    "sync:base_data_type": "curl --output ./proto/base_data_type.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/base_data_type.proto'",
    "sync:business": "curl --output ./proto/business.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/business.proto'",
    "sync:client": "curl --output ./proto/client.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/client.proto'",
    "sync:core_functionality": "curl --output ./proto/core_functionality.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/core_functionality.proto'",
    "sync:dictionary": "curl --output ./proto/dictionary.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/dictionary.proto'",
    "sync:enrollment": "curl --output ./proto/enrollment.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/enrollment.proto'",
    "sync:point_of_sales": "curl --output ./proto/point_of_sales.proto -L 'https://raw.githubusercontent.com/adempiere/adempiere-gRPC-Server/master/src/main/proto/point_of_sales.proto'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adempiere/gRPC-API.git"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/erpcya/gRPC-API/issues"
  },
  "homepage": "https://github.com/erpcya/gRPC-API#readme",
  "dependencies": {
    "@grpc/grpc-js": "1.3.7",
    "google-protobuf": "3.17.3"
  },
  "devDependencies": {
    "grpc-tools": "1.11.2"
  },
  "engines": {
    "node": ">= 10.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "compiere",
    "adempiere",
    "erp",
    "grpc",
    "webstore",
    "java",
    "web"
  ]
}
